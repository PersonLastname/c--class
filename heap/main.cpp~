using namespace std;

int main() {
  
  int numbers[50]; //sets up my array with a file full of numbers
  ifstream Numbers;
  Numbers.open("numbers.txt");
  int i = 0;
  while (i < 50) {
    Numbers >> numbers[i];
    i = i + 1;
  }

  int healp[50];

  
