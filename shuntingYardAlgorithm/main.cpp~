#include <iostream>
#include <cstring>
#include <cctype>
#include <stdio.h>
#include <ctype.h>

#include "Node.h"

using namespace std;

void add(Node * & head, char newData);
void print(Node * next);

int main() {
  
  char input[50];
  bool running = true;
  Node * head = new Node();
  Node * tail = new Node();
  head->setNext(tail);
  
}

void print(Node * next) { //calls itself until it reaches the end (when the next is null)
  if (next != NULL) {
    cout << endl;
    cout << "-\t-\t-" << endl;
    cout << "Contained char: ";
    cout << next -> getData();

    print(next -> getNext());

  }
}
