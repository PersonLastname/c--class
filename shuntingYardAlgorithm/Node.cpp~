#include "Node.h"
Node::Node(){
  data = NULL;
  next = NULL;
}
Node::~Node(){
  delete data;
}
void Node::setNext(Node* newNext){
  next=newNext;
}
Node*  Node::getNext(){
  return next;
}
void Node::setData(char newData){
  data = newData;
}
char  Node::getData(){
  return data;
}
